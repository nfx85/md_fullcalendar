{
    "name": "mediadreams/md_fullcalendar",
    "type": "typo3-cms-extension",
    "description": "This extension brings the FullCalendar.io with switchable views for month, week and day to ext:calendarize. It is also possible to filter calendars by categories.",
    "license": "GPL-2.0-or-later",
    "version": "4.0.0",
    "keywords": [
        "TYPO3",
        "extension",
        "calendar",
        "fullcalendar",
        "cal",
        "time"
    ],
    "homepage": "http://www.mediadreams.org",
    "authors": [
        {
            "name": "Christoph Daecke",
            "role": "Developer",
            "homepage": "http://www.mediadreams.org"
        }
    ],
    "support": {
        "issues": "https://github.com/cdaecke/md_fullcalendar/issues",
        "source": "https://github.com/cdaecke/md_fullcalendar"
    },
    "require": {
        "typo3/cms-core": "^11.5 || ^12.4",
        "lochmueller/calendarize": "^13"
    },
    "autoload": {
        "psr-4": {
            "Mediadreams\\MdFullcalendar\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "md_fullcalendar"
        }
   }
}
